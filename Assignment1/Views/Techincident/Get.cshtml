@model Assignment1.Models.Technicians
@{
    string title = "Get Techincident";
    ViewBag.Title = title;
}
@section header{
    <div class="Incidents-container p-3">
        <h1>@ViewBag.name</h1>
        <h2>@title</h2>
        <div class="box Get">
            <span></span>
            <div class="content">
                <h1>@ViewBag.error</h1>
                <form method="get">
                    <div class="form-group row ">
                        <div class="m-5 w-100">
                            <select asp-for="TechniciansId" class="form-control"
                                    asp-items="@(new SelectList(ViewBag.Technicians, "TechniciansId", "TName"))">
                                <option value="">Select a Technician</option>
                            </select>
                        </div>

                        <div class="form-group row">
                            <div class="col-5 offset-4 row">
                                <table class="col-5 offset-4 row">
                                    <tr class="col-5">
                                        <td><input type="submit" asp-action="List" class="btn btn-outline-success border-success text-right" value="Select"></td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                    </div>

                </form>
            </div>
        </div>
    </div>
}
