@{
    ViewData["Title"] = "Home Page";

}

@section header{

    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>@ViewData["Title"] - SportsPro</title>
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
        <link rel="stylesheet" href="~/css/site.css" />
        
    </head>

    <h1 class="display-4">SportsPro Technical Support</h1>
    <p>Sports management software for the sports enthusiast</p>
    <div class="index-container p-3">
        <div class="box">
            <span></span>
            <div class="content">
                <h2>Admin</h2>
                <table class="table">
                    <thead>
                        <tr>
                            <th><a asp-controller="Products" asp-action="Index"> Manage Products</a></th>
                        </tr>
                        <tr>
                            <th><a asp-controller="Technicians" asp-action="Index"> Manage Technicians</a></th>
                        </tr>
                        <tr>
                            <th><a asp-controller="Customers" asp-action="Index"> Manage Customers</a></th>
                        </tr>
                        <tr>
                            <th><a asp-area="Incidents" asp-action="Index"> Manage Incidents</a></th>
                        </tr>
                        <tr>
                            <th><a asp-controller="Registrations" asp-action="GetCustomer"> Manage Registrations</a></th>
                        </tr>
                    <thead>
                </table>
            </div>
        </div>


        <div class="box">
            <span></span>
            <div class=" content">
                <h2>Technicians</h2>
                <table class="table ">
                    <thead>
                        <tr>
                            <th><a asp-controller="Techincident" asp-action="Get"> Update Incident</a></th>
                        </tr>

                    </thead>
                </table>
            </div>
        </div>
    </div>
}